<?xml version="1.0" encoding="utf-8"?>
<androidx.constraintlayout.widget.ConstraintLayout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="match_parent"
    android:layout_marginLeft="5px"
    android:layout_marginRight="5px"
    android:layout_marginBottom="5px"
    android:background="#00000000">

    <LinearLayout
        android:layout_width="fill_parent"
        android:layout_height="fill_parent"
        android:background="#00000000"
        android:orientation="vertical"
        tools:ignore="MissingConstraints">

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="10px"
            android:layout_marginTop="10px"
            android:layout_marginRight="10px"
            android:layout_weight="1.7"
            android:background="#00000000"
            tools:ignore="MissingConstraints">

            <ImageView
                android:id="@+id/hero_info1"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginLeft="5px"
                android:layout_marginRight="5px"
                android:layout_weight="1"
                android:scaleType="centerCrop"
                android:src="@mipmap/_888"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/hero_info2"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginLeft="5px"
                android:layout_marginRight="5px"
                android:layout_weight="1"
                android:scaleType="centerCrop"
                android:src="@mipmap/_888"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/hero_info3"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginLeft="5px"
                android:layout_marginRight="5px"
                android:layout_weight="1"
                android:scaleType="centerCrop"
                android:src="@mipmap/_888"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/hero_info4"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginLeft="5px"
                android:layout_marginRight="5px"
                android:layout_weight="1"
                android:scaleType="centerCrop"
                android:src="@mipmap/_888"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

            <ImageView
                android:id="@+id/hero_info5"
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_marginLeft="5px"
                android:layout_marginRight="5px"
                android:layout_weight="1"
                android:scaleType="centerCrop"
                android:src="@mipmap/_888"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="fill_parent"
            android:layout_height="fill_parent"
            android:layout_marginLeft="5px"
            android:layout_marginTop="-3px"
            android:layout_marginRight="5px"
            android:layout_marginBottom="10px"
            android:layout_weight="1.2"
            android:background="#00000000"
            android:orientation="vertical"
            tools:ignore="MissingConstraints">

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="100px"
                android:layout_weight="1.75"
                android:background="#00000000"
                tools:ignore="MissingConstraints">

                <ProgressBar
                    android:id="@+id/hero_hp1"
                    style="@android:style/Widget.Holo.ProgressBar.Horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="5px"
                    android:layout_marginRight="5px"
                    android:layout_weight="1"
                    android:progressDrawable="@drawable/max_map_hero_info_hp"
                    android:secondaryProgress="100" />

                <ProgressBar
                    android:id="@+id/hero_hp2"
                    style="@android:style/Widget.Holo.ProgressBar.Horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="5px"
                    android:layout_marginRight="5px"
                    android:layout_weight="1"
                    android:progressDrawable="@drawable/max_map_hero_info_hp"
                    android:secondaryProgress="100" />

                <ProgressBar
                    android:id="@+id/hero_hp3"
                    style="@android:style/Widget.Holo.ProgressBar.Horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="5px"
                    android:layout_marginRight="5px"
                    android:layout_weight="1"
                    android:progressDrawable="@drawable/max_map_hero_info_hp"
                    android:secondaryProgress="100" />

                <ProgressBar
                    android:id="@+id/hero_hp4"
                    style="@android:style/Widget.Holo.ProgressBar.Horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="5px"
                    android:layout_marginRight="5px"
                    android:layout_weight="1"
                    android:progressDrawable="@drawable/max_map_hero_info_hp"
                    android:secondaryProgress="100" />

                <ProgressBar
                    android:id="@+id/hero_hp5"
                    style="@android:style/Widget.Holo.ProgressBar.Horizontal"
                    android:layout_width="match_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="5px"
                    android:layout_marginRight="5px"
                    android:layout_weight="1"
                    android:progressDrawable="@drawable/max_map_hero_info_hp"
                    android:secondaryProgress="100" />
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="#00000000"
                tools:ignore="MissingConstraints">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="2px"
                    android:layout_marginRight="2px"
                    android:layout_weight="1"
                    android:background="@drawable/shape_5dp_max_cd"
                    android:orientation="vertical"
                    tools:ignore="MissingConstraints">

                    <LinearLayout
                        android:id="@+id/linear_layout_hero_max_cd1"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="2px"
                        android:layout_marginRight="2px"
                        android:layout_weight="1"
                        android:background="@drawable/shape_5dp_max_cd"
                        android:orientation="horizontal"
                        tools:ignore="MissingConstraints">

                        <TextView
                            android:id="@+id/hero_max_cd1"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="0"
                            android:textColor="#a6afbc"
                            android:textSize="30px" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linear_layout_hero_max_cd2"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="2px"
                    android:layout_marginRight="2px"
                    android:layout_weight="1"
                    android:background="@drawable/shape_5dp_max_cd"
                    tools:ignore="MissingConstraints">

                    <TextView
                        android:id="@+id/hero_max_cd2"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="#a6afbc"
                        android:textSize="30px" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linear_layout_hero_max_cd3"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="2px"
                    android:layout_marginRight="2px"
                    android:layout_weight="1"
                    android:background="@drawable/shape_5dp_max_cd"
                    tools:ignore="MissingConstraints">

                    <TextView
                        android:id="@+id/hero_max_cd3"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="#a6afbc"
                        android:textSize="30px" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linear_layout_hero_max_cd4"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="2px"
                    android:layout_marginRight="2px"
                    android:layout_weight="1"
                    android:background="@drawable/shape_5dp_max_cd"
                    tools:ignore="MissingConstraints">

                    <TextView
                        android:id="@+id/hero_max_cd4"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="#a6afbc"
                        android:textSize="30px" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linear_layout_hero_max_cd5"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="2px"
                    android:layout_marginRight="2px"
                    android:layout_weight="1"
                    android:background="@drawable/shape_5dp_max_cd"
                    tools:ignore="MissingConstraints">

                    <TextView
                        android:id="@+id/hero_max_cd5"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="#a6afbc"
                        android:textSize="30px" />

                </LinearLayout>
            </LinearLayout>

            <LinearLayout
                android:layout_width="fill_parent"
                android:layout_height="fill_parent"
                android:layout_weight="1"
                android:background="#00000000"
                tools:ignore="MissingConstraints">

                <LinearLayout
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="2px"
                    android:layout_marginRight="2px"
                    android:layout_weight="1"
                    android:background="@drawable/shape_5dp_cd"
                    android:orientation="vertical"
                    tools:ignore="MissingConstraints">

                    <LinearLayout
                        android:id="@+id/linear_layout_hero_cd1"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_marginLeft="2px"
                        android:layout_marginRight="2px"
                        android:layout_weight="1"
                        android:background="@drawable/shape_5dp_cd"
                        android:orientation="horizontal"
                        tools:ignore="MissingConstraints">

                        <TextView
                            android:id="@+id/hero_cd1"
                            android:layout_width="fill_parent"
                            android:layout_height="fill_parent"
                            android:layout_weight="1"
                            android:gravity="center"
                            android:text="0"
                            android:textColor="#11243B"
                            android:textSize="30px" />
                    </LinearLayout>

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linear_layout_hero_cd2"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="2px"
                    android:layout_marginRight="2px"
                    android:layout_weight="1"
                    android:background="@drawable/shape_5dp_cd"
                    tools:ignore="MissingConstraints">

                    <TextView
                        android:id="@+id/hero_cd2"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="#11243B"
                        android:textSize="30px" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linear_layout_hero_cd3"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="2px"
                    android:layout_marginRight="2px"
                    android:layout_weight="1"
                    android:background="@drawable/shape_5dp_cd"
                    tools:ignore="MissingConstraints">

                    <TextView
                        android:id="@+id/hero_cd3"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="#11243B"
                        android:textSize="30px" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linear_layout_hero_cd4"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="2px"
                    android:layout_marginRight="2px"
                    android:layout_weight="1"
                    android:background="@drawable/shape_5dp_cd"
                    tools:ignore="MissingConstraints">

                    <TextView
                        android:id="@+id/hero_cd4"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="#11243B"
                        android:textSize="30px" />

                </LinearLayout>

                <LinearLayout
                    android:id="@+id/linear_layout_hero_cd5"
                    android:layout_width="fill_parent"
                    android:layout_height="fill_parent"
                    android:layout_marginLeft="2px"
                    android:layout_marginRight="2px"
                    android:layout_weight="1"
                    android:background="@drawable/shape_5dp_cd"
                    tools:ignore="MissingConstraints">

                    <TextView
                        android:id="@+id/hero_cd5"
                        android:layout_width="fill_parent"
                        android:layout_height="fill_parent"
                        android:layout_weight="1"
                        android:gravity="center"
                        android:text="0"
                        android:textColor="#11243B"
                        android:textSize="30px" />

                </LinearLayout>
            </LinearLayout>


        </LinearLayout>

    </LinearLayout>

</androidx.constraintlayout.widget.ConstraintLayout>